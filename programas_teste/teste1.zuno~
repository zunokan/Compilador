inteiro soma_3 ( inteiro x, inteiro y, inteiro z){
        inteiro aux = x + y + z;
        retorne aux;
}

procedimento imprime (caractere y){
        caractere texto =  y;
		escreva(y);
}

inteiro faixa (flutuante b){
	se ((b > 0) && (b < 5)){
		retorne 1;	
	} senao se ((b > 5.5) && (b <= 10)){
		retorne 2;
	} senao se ((b> 10.5) || (b < 10.9)){
		retorn 3;
	} senao {
		retorne 10;	
	}
}

programa { 
    inteiro flag = 0		
	flutuante x = 1;
    inteiro teste = faixa(x);
    enquanto (flag < 10) {
		escreva(x);
		faixa = faixa + ((2*2) - 1);
        flag = flag + 1;
    }
} 


