
programa { 
    inteiro flag = 0;		
	flutuante x = 1;
	inteiro teste = 12;
	string faixa = "10";
	se ((x > 0) && (x < 5)){
		escreva("1");	
	} senao se ((x > 5.5) && (x <= 10)){
		escreva("2");
	} senao se ((x> 10.5) || (x < 10.9)){
		escreva("3");
	} senao {
		escreva("4");
	}
    
    enquanto (flag < 10) {
		escreva("Coxinha");
		faixa = faixa + ((2*2) - 1);
        flag = flag + 1;
    }
} 


